{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "lint": {
      "outputs": [],
      "cache": false
    },
    "test": {
      "outputs": [],
      "dependsOn": ["^build"]
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "publish": {
      "dependsOn": ["build"],
      "cache": false
    },
    "storybook": {
      "outputs": ["build/**"]
    }
  }
}